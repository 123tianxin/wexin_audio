 <view>
    <swiper indicator-dots="true" autoplay="true" interval="5000">
        <swiper-item>
            <image src="{{list_key[0].list_img}}"></image>
        </swiper-item>
        <swiper-item>
            <image src="/images/ui/swiper1.bmp"></image>
        </swiper-item>
        <swiper-item>
            <image src="https://image.baidu.com/search/down?tn=download&ipn=dwnl&word=download&ie=utf8&fr=result&url=http%3A%2F%2Fimg.zcool.cn%2Fcommunity%2F0142a6590aa1b6a801214550342477.jpg%402o.jpg&thumburl=https%3A%2F%2Fss0.bdstatic.com%2F70cFvHSh_Q1YnxGkpoWK1HF6hhy%2Fit%2Fu%3D1077230490%2C59001420%26fm%3D27%26gp%3D0.jpg"></image>
        </swiper-item>
    </swiper>
    
    <block wx:for="{{list_key}}" wx:for-item="item" wx:key="date" wx:for-index="index">
      <view  class="post-container" >
          <text class="post-title">{{item.title}}<text class="post-date">{{item.date}}</text></text>
          <view id="{{item.objectId}}" bindtap="onBindTap">
            <image  class="post-image" src="{{item.list_img}}"></image>
          </view>
          <text class="post-content">{{item.list_content}}</text>
      </view>
   </block>
   
</view>
